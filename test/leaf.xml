<leaf name="lateral" subType="1">
    <parameter name="gf" value="1" dev="0"/>
    <!--Growth function number [1]-->
    <parameter name="lnf" value="0"/>
    <!--Type of inter-branching distance (0 homogeneous, 1 linear inc, 2 linear dec, 3 exp inc, 4 exp dec)-->
    <parameter name="tropismT" value="1"/>
    <!--Type of leaf tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
    <parameter name="a" value="1" dev="0"/>
    <!--Leaf width [cm]-->
    <parameter name="dx" value="0.25"/>
    <!--Axial resolution [cm] (maximal segment size)-->
    <parameter name="k" value="10" dev="0"/>
    <!--Maximal leaf length [cm]-->
    <parameter name="la" value="1.5" dev="0"/>
    <!--Apical zone [cm]-->
    <parameter name="lb" value="5.5" dev="0"/>
    <!--Basal zone [cm]-->
    <parameter name="ln" value="1.25" dev="0.12"/>
    <!--Inter-lateral distance [cm]-->
    <parameter name="r" value="1" dev="0"/>
    <!--Initial growth rate [cm day-1]-->
    <parameter name="rlt" value="1e+09" dev="0"/>
    <!--Root life time [day]-->
    <parameter name="theta" value="1.22" dev="0"/>
    <!--Angle between root and parent root [rad]-->
    <parameter name="tropismN" value="1"/>
    <!--Number of trials of leaf tropism-->
    <parameter name="tropismS" value="0.2"/>
    <!--Mean value of expected change of leaf tropism [1/cm]-->
    <parameter name="successor" number="0" type="4" percentage="0.40000001"/>
    <!--Sub type of lateral leaf veins-->
    <parameter name="successor" number="1" type="5" percentage="0.1"/>
    <!--Sub type of lateral leaf veins-->
    <parameter name="successor" number="2" type="6" percentage="0.5"/>
    <!--Sub type of lateral leaf veins-->
</leaf>
