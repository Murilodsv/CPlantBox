<?xml version="1.0" encoding="UTF-8"?>
<Plant name="maize" filetype="parameters">
<organ type="seed" subType="0">
    <parameter name="seedPos.x" value="0"/>
    <parameter name="seedPos.y" value="0"/>
    <parameter name="seedPos.z" value="-3"/>
    <parameter name="plantingdepth" value="3"/>
    <parameter name="firstB" value="0" dev="0"/>
    <parameter name="delayB" value="0" dev="0"/>
    <parameter name="maxB" value="0" dev="0"/>
    <parameter name="nC" value="2"/>
    <parameter name="nz" value="0"/>
    <parameter name="firstSB" value="14" dev="0"/>
    <parameter name="delaySB" value="0" dev="0"/>
    <parameter name="delayRC" value="7" dev="0"/>
    <parameter name="simulationTime" value="56" dev="0"/>
</organ>

<organ type="root" name="taproot" subType="1">
    <parameter name="lb" value="0.1" dev="0"/>
    <!--Basal zone [cm]-->
    <parameter name="la" value="0.1" dev="0"/>
    <!--Apical zone [cm];-->
    <parameter name="ln" value="1" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="lmax" value="1.3" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="nob" value="1.3666666666666665" dev="0"/>
    <parameter name="r" value="4.5" dev="0.45000000000000001"/>
    <parameter name="a" value="0.20000000000000001" dev="0.02"/>
    <parameter name="colorR" value="1"/>
    <parameter name="colorG" value="0"/>
    <parameter name="colorB" value="0"/>
    <parameter name="tropismT" value="1"/>
    <parameter name="tropismN" value="1"/>
    <parameter name="tropismS" value="0.261799"/>
    <parameter name="dx" value="0.25"/>
    <parameter name="theta" value="0" dev="0"/>
    <parameter name="rlt" value="1000000000" dev="0"/>
    <parameter name="gf" value="1"/>
    <parameter name="successor" number="0" type="0" percentage="1"/>
</organ>


<organ type="stem" name="mainstem" subType="1">
    <parameter name="lb" value="1" dev="0"/>
    <!--Basal zone [cm]-->
    <parameter name="la" value="0.1" dev="0"/>
    <!--Apical zone [cm];-->
    <parameter name="ln" value="1" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="lmax" value="1.3" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="nob" value="3.3333333333333335" dev="0"/>
    <parameter name="r" value="2.1000000000000001" dev="0"/>
    <parameter name="a" value="0.20000000000000001" dev="0.02"/>
    <parameter name="colorR" value="1"/>
    <parameter name="colorG" value="0"/>
    <parameter name="colorB" value="0"/>
    <parameter name="tropismT" value="0"/>
    <parameter name="tropismN" value="1"/>
    <parameter name="tropismS" value="0"/>
    <parameter name="dx" value="0.25"/>
    <parameter name="theta" value="0" dev="0"/>
    <parameter name="rlt" value="1000000000" dev="0"/>
    <parameter name="gf" value="1"/>
    <parameter name="successor" number="0" type="0" percentage="1"/>
</organ>


<organ type="leaf" name="lateral2" subType="2">
    <parameter name="lb" value="0.1" dev="0"/>
    <!--Basal zone [cm]-->
    <parameter name="la" value="0.1" dev="0"/>
    <!--Apical zone [cm];-->
    <parameter name="ln" value="1" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="lmax" value="1.3" dev="0"/>
    <!--Inter-lateral distance [cm];-->
    <parameter name="nob" value="0" dev="0"/>
    <parameter name="r" value="4" dev="0"/>
    <parameter name="a" value="0.02" dev="0.002"/>
    <parameter name="colorR" value="1"/>
    <parameter name="colorG" value="1"/>
    <parameter name="colorB" value="1"/>
    <parameter name="tropismT" value="1"/>
    <parameter name="tropismN" value="3"/>
    <parameter name="tropismS" value="0"/>
    <parameter name="dx" value="0.25"/>
    <parameter name="theta" value="0.5" dev="0"/>
    <parameter name="rlt" value="1000000000" dev="0"/>
    <parameter name="gf" value="1"/>
</organ>


</Plant>
